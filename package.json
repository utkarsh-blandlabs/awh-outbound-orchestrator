{
  "name": "awh-outbound-orchestrator",
  "version": "1.0.0",
  "description": "Replacing Zapier for American Way Health's outbound call automation",
  "main": "build/index.js",
  "author": "Utkarsh Jaiswal",
  "license": "ISC",
  "keywords": [
    "awh",
    "bland",
    "Convoso",
    "outbound",
    "orchestrator"
  ],
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.json src/index.ts",
    "build": "tsc",
    "start": "node build/index.js",
    "lint": "echo 'No linting configured yet'",
    "test": "echo 'No tests configured yet'",
    "backfill": "node --max-old-space-size=2048 --expose-gc backfill-from-bland-optimized.js",
    "backfill:dry-run": "node --max-old-space-size=2048 --expose-gc backfill-from-bland-optimized.js --dry-run",
    "backfill:old": "node --expose-gc backfill-from-bland.js",
    "report:breakdown": "node get-detailed-breakdown.js",
    "report:date-range": "node get-date-range-report.js",
    "report:total": "node get-total-leads.js"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "dotenv": "^17.2.3",
    "express": "^4.19.0",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": "20.x"
  }
}
